{
    "name": "laraxot/module_chart_fila3",
    "description": "chart module that manages your data and represents them in graphs",
    "keywords": [
        "laraxot",
        "laravel",
        "filament",
        "module_chart",
        "Laravel",
        "Chart module",
        "Charts"
    ],
    "homepage": "https://github.com/laraxot/module_chart_fila3",
    "license": "MIT",
    "authors": [{
        "name": "Marco Sottana",
        "email": "marco.sottana@gmail.com"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Chart\\Providers\\ChartServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Chart\\": "app/"
        }
    },
    "require": {
        "amenadiel/jpgraph": "^4.1"
    },
    "require_comment": {
        "laraxot/module_xot_fila3": "*",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev_comment": {
        "driftingly/rector-laravel": "^0.26.2",
        "enlightn/enlightn": "^2.5",
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "*",
        "larastan/larastan": "^2.6",
        "nunomaduro/phpinsights": "^2.9",
        "orchestra/testbench": "^9.4",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-arch": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpstan/phpstan-deprecation-rules": "*",
        "phpstan/phpstan-phpunit": "*",
        "psalm/plugin-laravel": "*",
        "rector/phpstan-rules": "*",
        "rector/rector": "*",
        "spatie/laravel-ray": "*",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "tightenco/duster": "^2.2",
        "vimeo/psalm": "^5.15"
    },
    "repositories": [ {
        "type": "path",
        "url": "../Xot"
    },
    {
        "type": "path",
        "url": "../Tenant"
    },
    {
        "type": "path",
        "url": "../UI"
    }],
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
